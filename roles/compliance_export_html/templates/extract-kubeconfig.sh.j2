#!/bin/bash
IMPORT_FILE="$1"
if [ -f "${IMPORT_FILE}" ]; then
  KUBECONFIG_B64=$(cat "${IMPORT_FILE}" | awk '/name: "bootstrap-hub-kubeconfig"/,/^---/' | grep 'kubeconfig:' | sed -E 's/.*kubeconfig:[[:space:]]*"([^"]+)".*/\1/' | head -1)
  if [ -n "${KUBECONFIG_B64}" ] && [ ${#KUBECONFIG_B64} -gt 100 ]; then
    echo "${KUBECONFIG_B64}" | base64 -d 2>/dev/null || echo ''
  else
    echo ''
  fi
else
  echo ''
fi
